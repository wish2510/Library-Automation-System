<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
    <class name="vo.publisherVO" table="publisher">
        
        <id name="publisherID" type="long" column="PublisherID">
        <generator class="increment"/>
        </id>
        
        <property name="publisherName">
        <column name="PublisherName"/>
        </property>
        
        <property name="publisherLoc">
        <column name="PublisherLoc"/>
        </property>
         
    </class>

</hibernate-mapping>

