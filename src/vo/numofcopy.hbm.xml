<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 
 <class name="vo.numofcopyVO" table="numofcopy">
 
  <id name="numofcopyID" type="long" column="NumOfCopyID" >
  <generator class="increment"/>
  </id>
	
  <property name="branchID">
  <column name="BranchID"/>
  </property>
  
  <property name="bookID">
  <column name="BookID"/>
  </property>
  
  <property name="copy">
  <column name="Copy"/>
  </property>
  
 </class>

</hibernate-mapping>

